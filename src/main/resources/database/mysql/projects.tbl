CREATE TABLE `projects` (
  `id` int(13) ,
  `url` varchar(512) ,
  `owner_id` int(13) ,
  `name` varchar(512) ,
  `description` varchar(512) ,
  `language` varchar(512) ,
  `created_at` varchar(32),
  `ext_ref_id` varchar(32),
  `forked_from` int(13) ,
  `deleted` tinyint(1)
) ENGINE=InnoDB DEFAULT CHARSET=utf8
