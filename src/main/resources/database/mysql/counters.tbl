CREATE TABLE `counters` (
  `id` int(13) ,
  `day` date ,
  `commit_comments` int(13) ,
  `commit_parents` int(13) ,
  `commits` int(13) ,
  `followers` int(13) ,
  `organization_members` int(13) ,
  `projects` int(13) ,
  `users` int(13) ,
  `issues` int(13) ,
  `pull_requests` int(13) ,
  `issue_comments` int(13) ,
  `pull_request_comments` int(13) ,
  `pull_request_history` int(13) ,
  `watchers` int(13) ,
  `forks` int(13)
) ENGINE=InnoDB DEFAULT CHARSET=utf8