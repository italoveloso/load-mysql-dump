 CREATE TABLE `users` (
  `id` int(13) ,
  `login` varchar(512) ,
  `name` varchar(512) ,
  `company` varchar(512) ,
  `location` varchar(512) ,
  `email` varchar(512) ,
  `created_at` varchar(32),
  `ext_ref_id` varchar(32),
  `type` varchar(512) ,
  `fake` tinyint(1)
) ENGINE=InnoDB DEFAULT CHARSET=utf8